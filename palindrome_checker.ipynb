   "source": [
    "def is_palindrome(text):\n",
    "    text = text.lower()\n",
    "    return text == text[::-1]  \n",
    "\n",
    "word = \"madam\"\n",
    "if is_palindrome(word):\n",
    "    print(f\"'{word}' Palindrome hai ✓\")\n",
    "else:\n",
    "    print(f\"'{word}' Palindrome nahi hai ✗\")"
   ]
